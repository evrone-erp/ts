{
  "auth": {
    "params": {
      "client_id": "env.CLIENT_ID",
      "response_type": "token"
    },
    "url": "https://oauth.yandex.ru/authorize"
  },
  "options": {
    "headers": {}
  },
  "trackerUrl": "https://tracker.yandex.ru/",
  "jiraAuth": {
    "params": {
      "audience": "api.atlassian.com",
      "client_id": "env.JIRA_CLIENT_ID",
      "scope": [
        "read:me",
        "read:account",
        "read:jira-work",
        "read:jira-user",
        "write:jira-work",
        "manage:jira-project",
        "manage:jira-configuration",
        "offline_access"
      ],
      "response_type": "code",
      "prompt": "consent"
    },
    "url": "https://auth.atlassian.com/authorize"
  }
}
